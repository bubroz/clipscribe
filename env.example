# ClipScribe 2.0 Environment Variables
# Copy this file to .env and fill in your values

# Google API Key for Gemini 2.5 Flash
# Get from: https://aistudio.google.com/apikey
GOOGLE_API_KEY=your_google_api_key_here

# Optional: Maximum video duration in seconds (default: 3600 = 1 hour)
MAX_VIDEO_DURATION=14400

# Optional: Video retriever settings
VIDEO_RETRIEVER_ENABLED=true
VIDEO_MAX_RESULTS=15